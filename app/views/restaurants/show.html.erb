<h1><%= @restaurant.name%></h1>
<p><%= @restaurant.address%></p>
<p><%= @restaurant.phone_number%></p>
<p><%= @restaurant.category%></p>

<% @restaurant.reviews.each do |review|%>
<p><%= review.content%></p>
<% end %>

<p><%= link_to 'Add a review for this restaurant', new_restaurant_review_path(@restaurant)%></p>
<p><%= link_to 'All restaurants', restaurants_path %></p>
